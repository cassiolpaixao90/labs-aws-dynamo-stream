{
	"name": "labs-aws-dynamo-streams",
	"version": "1.0.0",
	"description": "Sample app serverless-aws to save data in dynamodb with trigger",
	"scripts": {
		"debug:win": "SET SLS_DEBUG=* && node --inspect %USERPROFILE%\\AppData\\Roaming\\npm\\node_modules\\serverless\\bin\\serverless offline -s dev",
		"test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
		"test:verbose": "jest --passWithNoTests --runInBand",
		"test:unit": "npm test -- --watch -c jest-unit-config.js",
		"test:integration": "npm test -- --watch -c jest-integration-config.js",
		"test:staged": "npm test -- --findRelatedTests",
		"test:ci": "npm test -- --coverage"
	},
	"keywords": [
		"node",
		"typescript",
		"aws",
		"serverless",
		"clean architecture",
		"jest",
		"babel",
		"dynamodb",
		"trigger"
	],
	"author": "",
	"license": "MIT",
